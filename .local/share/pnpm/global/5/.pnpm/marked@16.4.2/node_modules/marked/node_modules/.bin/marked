#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/ubuntu/.local/share/pnpm/global/5/.pnpm/marked@16.4.2/node_modules/marked/bin/node_modules:/home/ubuntu/.local/share/pnpm/global/5/.pnpm/marked@16.4.2/node_modules/marked/node_modules:/home/ubuntu/.local/share/pnpm/global/5/.pnpm/marked@16.4.2/node_modules:/home/ubuntu/.local/share/pnpm/global/5/.pnpm/node_modules"
else
  export NODE_PATH="/home/ubuntu/.local/share/pnpm/global/5/.pnpm/marked@16.4.2/node_modules/marked/bin/node_modules:/home/ubuntu/.local/share/pnpm/global/5/.pnpm/marked@16.4.2/node_modules/marked/node_modules:/home/ubuntu/.local/share/pnpm/global/5/.pnpm/marked@16.4.2/node_modules:/home/ubuntu/.local/share/pnpm/global/5/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/marked.js" "$@"
else
  exec node  "$basedir/../../bin/marked.js" "$@"
fi
